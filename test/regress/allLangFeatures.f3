c -= 5;

var b : Boolean = true;

// PREFIX_INCREMENT
++c;
// POSTFIX_INCREMENT
c++;
// PREFIX_DECREMENT
--c;
// POSTFIX_DECREMENT
c--;

// UNARY_MINUS
-c;

// LOGICAL_COMPLEMENT
not b;

// CONDITIONAL_AND
b and true;

// CONDITIONAL_OR
b or true;

// DIVIDE
c / 2;

// GREATER_THAN
c > 10;

