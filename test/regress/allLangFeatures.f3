} catch (ex) {

}
var c  : Integer = 0;
// WHILE_LOOP
while (c < 10) {
    c++;
    if (c > 5) {
// BREAK
        break;
    } else {
// CONTINUE
        continue;
    }

}

// INSTANCE_OF
x instanceof AllTrees;
// PARENTHESIZED
// ???

